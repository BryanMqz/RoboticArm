    <node name ="GoalPlanner" pkg="path_planner" type="GoalPlanner.py" />

    <node name="listener2" pkg="roscpp_tutorials" type="listener" launch-prefix="bash -c 'sleep $(arg node_start_delay); $2 $@' " />


    <node name ="solution_template" pkg="pick_place" type="solution_template.py" />
